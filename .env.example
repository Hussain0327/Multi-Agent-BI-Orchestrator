# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5-nano

# LangSmith Configuration (for tracing and monitoring)
# Get your API key from: https://smith.langchain.com/settings
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=business-intelligence-orchestrator
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Optional: Disable tracing if needed (set to false)
# LANGCHAIN_TRACING_V2=false

# Cache Configuration (Redis for production, file for dev)
CACHE_ENABLED=true
CACHE_BACKEND=redis
REDIS_URL=redis://localhost:6379/0
CACHE_NAMESPACE=bi
